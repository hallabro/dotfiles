download-path "~/podcasts/%n"
show-read-feeds no

color listnormal color7 color0
color listnormal_unread color7 color8
color listfocus color18 color7
color listfocus_unread color18 color7
color info color7 color19
color article color7 color19

bind-key j down tagselection
bind-key k up tagselection

bind-key j down feedlist
bind-key k up feedlist

bind-key j next articlelist
bind-key k prev articlelist

bind-key J next-feed articlelist
bind-key K prev-feed articlelist

bind-key j down article
bind-key k up article
